
#content-left

  %h1 
    New Finish

  %ul
    - @finishes.each do |fin| 
      %li
        = link_to  fin.title, edit_admin_finish_path(fin)

#content-center.TwoColumnFix

  = form_for @finish, :url => admin_finishes_path, :method => :post do |f| 

    = f.text_field :title

    = f.submit 'Add Finish', :id => "add_finish", :class => "submit"