
#content-left

  %h1
    Materials Manager

  %ul
    - @materials.each do |material| 
      %li
        = link_to  material.title, edit_admin_material_path(material)

#content-center.TwoColumnFix

  = form_for @material, :url => admin_materials_path do |mf|
    = mf.text_field :title, :placeholder => 'Create New Material'
    = mf.submit :id => :create_material



